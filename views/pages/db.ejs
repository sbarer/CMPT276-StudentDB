<!DOCTYPE html>
<html>
<head>
  <% include ../partials/header.ejs %>
</head>

<body>

<% include ../partials/nav.ejs %>

<div class="container">
<h2>Database Results</h2>

<ul>
    <% results.forEach(function(r) { %>
        <li><%= r.id %> - <%= r.name %>  - <%= r.weight %>  - <%= r.height %> - <%= r.hair_color %> - <%= r.gpa %> - <%= r.major %> - <%= r.age %></li>
    <% }); %>
</ul>

</div>

</body>
</html>
